<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sn.groupeisi.examfx.controllers.LoginController">
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="206.0">
         <children>
            <ImageView fitHeight="412.0" fitWidth="263.0" layoutY="-3.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/ma%20photo.jpg" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
      <Text layoutX="358.0" layoutY="68.0" strokeType="OUTSIDE" strokeWidth="0.0" text="CONNEXION">
         <font>
            <Font size="24.0" />
         </font>
      </Text>
      <Text layoutX="302.0" layoutY="117.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Email">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <Text layoutX="302.0" layoutY="213.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Mot de passe">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <TextField fx:id="emailTfd" layoutX="302.0" layoutY="144.0" prefHeight="25.0" prefWidth="250.0" promptText="Email" />
      <PasswordField fx:id="passwordTfd" layoutX="302.0" layoutY="244.0" prefHeight="25.0" prefWidth="250.0" promptText="mot de passe" />
      <Button fx:id="connexionBtn" layoutX="316.0" layoutY="310.0" mnemonicParsing="false" onAction="#getLogin" text="Se connecter" />
      <Button fx:id="registerBtn" layoutX="453.0" layoutY="310.0" mnemonicParsing="false" onAction="#getRegister" prefHeight="25.0" prefWidth="85.0" text="S'inscrire" />
